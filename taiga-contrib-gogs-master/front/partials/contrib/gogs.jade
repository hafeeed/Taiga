|module.run(['$templateCache', ($templateCache) ->
|  $templateCache.put('contrib/gogs', '''
div(contrib-gogs-webhooks, ng-controller="ContribGogsAdminController as ctrl")
    header
        h1(tg-main-title)

    form
        fieldset
            label(for="secret-key") Secret key
            input(type="text", name="secret-key", ng-model="gogs.secret", placeholder="Secret key", id="secret-key")

        fieldset
            .select-input-text(tg-select-input-text)
                div
                    label(for="payload-url") Payload URL
                    .field-with-option
                        input(type="text", ng-model="gogs.webhooks_url", name="payload-url", readonly="readonly", placeholder="Payload URL", id="payload-url")
                        .option-wrapper.select-input-content
                            .icon.icon-copy
                    .help-copy Copy to clipboard: Ctrl+C

        button(type="submit", class="hidden")
        a.button-green.submit-button(href="", title="Save")
            span Save

    a.help-button(href="https://taiga.io/support/gogs-integration/", target="_blank")
        span.icon.icon-help
        span Do you need help? Check out our support page!
|''')]);
